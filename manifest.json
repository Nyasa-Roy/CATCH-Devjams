{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "Categorize Instagram saved posts and Google bookmarks.",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://www.instagram.com/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
