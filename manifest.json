{
    "manifest_version": 3,
    "name": "Data Categorizer",
    "version": "1.0",
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.instagram.com/*", "*://*.google.com/bookmarks/*"],
        "js": ["content.js"]
      }
    ]
  }
  